import turtle
tina = turtle.Turtle()
tina.shape('turtle')

tina.penup()
tina.write("coded by donhdd")

tina.goto(100,100)
tina.write("Hello")

tina.goto(-100,-100)
tina.write("Im 92")

tina.goto(100,-100)
tina.write("Your mom is gay")

tina.goto(-100,100)
tina.write("I sang a dickriders song")

tina.goto(-100, 0)
